#!/bin/sh

LUAROCKS_SYSCONFDIR='/etc/tarantool/rocks' exec '/usr/bin/tarantool' -e 'package.path="/tarantool-api/.rocks/share/tarantool/?.lua;/tarantool-api/.rocks/share/tarantool/?/init.lua;"..package.path;package.cpath="/tarantool-api/.rocks/lib/tarantool/?.so;"..package.cpath;local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("luatest","0.5.6-1")' '/tarantool-api/.rocks/share/tarantool/rocks/luatest/0.5.6-1/bin/luatest' "$@"
